<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.mawais123.hciproject11.final_complaintrgstr">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Congratulations. You have Sucessfully Registered..."
        android:textColor="#000000"
        android:id="@+id/textView" android:layout_gravity="center"
        android:textSize="15dp"
        android:textAlignment="center"/>
    <TextView
        android:layout_width="200dp"
        android:textColor="#000000"
        android:layout_height="40dp"
        android:text="Your Complaint Details"
        android:id="@+id/textView2"
        android:layout_below="@+id/email"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="46dp"
        android:layout_alignRight="@+id/textView"
        android:layout_alignEnd="@+id/textView" />


    <TextView
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:id="@+id/fname"
        android:textColor="#000000"
        android:layout_alignTop="@+id/textView3"
        android:layout_alignParentRight="true"
        android:textSize="15dp"/>

    <TextView
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:id="@+id/lname"
        android:textColor="#000000"
        android:layout_below="@+id/textView3"
        android:textSize="15dp"/>



    <TextView
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:id="@+id/email"
        android:textColor="#000000"
        android:layout_alignLeft="@+id/fname"
        android:textSize="15dp"
        android:layout_below="@+id/fname" />


    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Click Here to Welcome"
        android:id="@+id/login"
        android:textColor="#ff39d12d"
        android:textSize="13dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="About Us"
        android:id="@+id/button17"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/email"
        android:layout_alignStart="@+id/email" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact Us"
        android:id="@+id/button18"
        android:layout_alignBottom="@+id/button17"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Home"
        android:id="@+id/button19"
        android:layout_alignTop="@+id/button17"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>
