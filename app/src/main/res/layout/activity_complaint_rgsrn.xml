<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.mawais123.hciproject11.complaint_rgsrn">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Register Complaint"
        android:id="@+id/textView"
        android:layout_gravity="center"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="#000000"/>

    <TextView
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:text="Subject/Tittle"
        android:id="@+id/textView2"
        android:textColor="#000000"
        android:layout_below="@+id/textView"
        android:layout_alignParentLeft="true"
        android:textSize="15dp"/>

    <EditText
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:id="@+id/complaint"
        android:hint="tittle of complaint"
        android:textColor="#000000"
        android:layout_alignTop="@+id/textView2"
        android:layout_alignParentRight="true"
        android:textSize="15dp"/>

    <TextView
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:text="Institute"
        android:textColor="#000000"
        android:id="@+id/textView3"
        android:layout_below="@+id/textView2"
        android:layout_toLeftOf="@+id/textView"
        android:textSize="15dp"/>

    <EditText
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:id="@+id/lname"
        android:hint="Institute Name"
        android:layout_alignTop="@+id/textView3"
        android:layout_alignParentRight="true"
        android:textSize="15dp"/>

    <TextView
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:text="Town/UC"
        android:id="@+id/textView4"
        android:layout_below="@+id/textView3"
        android:layout_alignRight="@+id/textView3"
        android:textSize="15dp"/>

    <EditText
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:inputType="textEmailAddress"
        android:ems="10"
        android:id="@+id/email"
        android:hint="UC "
        android:layout_below="@+id/lname"
        android:layout_alignParentRight="true"
        android:textSize="15dp"/>

    <TextView
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:text="Employee Name"
        android:id="@+id/textView5"
        android:layout_below="@+id/textView4"
        android:layout_alignRight="@+id/textView4"
        android:textSize="15dp"/>

    <EditText
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:id="@+id/uname"
        android:hint="en"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignParentRight="true"
        android:textSize="15dp"/>

    <EditText
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/pword"
        android:hint="scale "
        android:layout_alignLeft="@+id/uname"
        android:layout_below="@+id/textView5"
        android:textSize="15dp"/>

    <TextView
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:text="Designation"
        android:id="@+id/textView6"
        android:layout_alignTop="@+id/pword"
        android:layout_alignRight="@+id/textView5"
        android:textSize="15dp"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:text="Register"
        android:id="@+id/register"
        android:background="@android:color/holo_blue_dark"
        android:textSize="13dp"
        android:layout_alignTop="@+id/bktologin"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="175dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:text="Cancel"
        android:background="@android:color/holo_blue_dark"
        android:id="@+id/bktologin"
        android:textSize="13dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:text="Institute Catg"
        android:id="@+id/textView8"
        android:textSize="15dp"
        android:layout_below="@+id/textView6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Govt."
        android:id="@+id/checkBox"
        android:checked="false"
        android:layout_alignTop="@+id/textView8"
        android:layout_centerHorizontal="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Private"
        android:id="@+id/checkBox2"
        android:checked="false"
        android:layout_alignTop="@+id/checkBox"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:text="Police Station"
        android:id="@+id/textView10"
        android:textSize="15dp"
        android:layout_alignRight="@+id/textView6"
        android:layout_alignEnd="@+id/textView6"
        android:layout_below="@+id/textView8" />

    <TextView
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:text="Evidence "
        android:id="@+id/textView12"
        android:textSize="15dp"
        android:layout_below="@+id/textView10"
        android:layout_toLeftOf="@+id/textView"
        android:layout_toStartOf="@+id/textView" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:inputType="textEmailAddress"
        android:ems="10"
        android:id="@+id/editText2"
        android:hint="UC "
        android:textSize="15dp"
        android:layout_below="@+id/checkBox"
        android:text="Near Police Station"
        android:layout_alignLeft="@+id/pword"
        android:layout_alignStart="@+id/pword" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Type Your Detail "
        android:id="@+id/textView13"
        android:layout_below="@+id/editText2"
        android:layout_alignLeft="@+id/editText2"
        android:layout_alignStart="@+id/editText2"
        android:layout_alignBottom="@+id/textView12"
        android:layout_alignRight="@+id/editText2"
        android:layout_alignEnd="@+id/editText2" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Upload"
        android:id="@+id/upload"
        android:layout_below="@+id/textView13"
        android:layout_alignRight="@+id/textView13"
        android:layout_alignEnd="@+id/textView13"
        android:layout_toRightOf="@+id/textView"
        android:layout_toEndOf="@+id/textView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Video/Audio/.PNG"
        android:id="@+id/textView11"
        android:layout_alignBottom="@+id/button15"
        android:layout_toLeftOf="@+id/button15"
        android:layout_toStartOf="@+id/button15" />


</RelativeLayout>





